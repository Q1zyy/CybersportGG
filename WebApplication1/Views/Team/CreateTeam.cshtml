@model WebApplication1.ViewModels.TeamViewModel
@{
    Layout = "_Layout";
}
<link rel="stylesheet" href="~/css/createteam.css" />
<body>
    <div class="container">
        <h1>Создание команды</h1>
        <form method="post" enctype="multipart/form-data">
            <div class="form-group">
                <label for="title">Загрузите фото:</label>
                <input required type="file" required asp-for="Image" />
            </div>
            <div class="form-group">
                <label for="title">Введите имя</label>
                <input required asp-for="Name"/>
            </div>
            <div class="form-group">
                <input type="submit" value="Отправить">
            </div>
        </form>
    </div>
</body>    